package com.evilcorp;

public class Main {
    public static void main(String[] args) {
        int[] staff = {2, 3, 2, 1, 3, 4, 2, 3, 4, 5, 2, 4, 4, 2};
        int weekCount = staff.length / 7;
        int[] week = new int [weekCount];
        for (int i = 0; i < week.length; i++) {
           int sum = 0;
        for (int x = 0; x < 7; x++) {
          sum = sum + staff[x + i * 7];
 }
            System.out.println(week[i]);
  {
