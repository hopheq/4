package com.evilcorp;

public class Main {
    public static void main(String[] args) {
        int[] x = new int[100];
        for (int i = 0; i < x.length; i++) {
            x[i] = i;
        }
        for (int i = 0; i < x.length; i++) {
            System.out.println("x[" + i + "] = " + x[i]);

        }
        }
    }
    
 
